
@{
    ViewData["Title"] = "UserList";
}
@model List<UserViewModelForAdmin>
<h1>Üye Listesi</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Kullanıcı Adı</th>
            <th>Email</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td><a class="btn btn-primary" asp-route="Admin" asp-controller="">Rol Ata</a></td>
            </tr>
        }
    </tbody>
</table>

